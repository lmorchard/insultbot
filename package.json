{
  "name": "insultbot",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "handler.js",
  "scripts": {
    "test": "npm-run-all test:*",
    "test:js": "mocha --require lib/test-setup --recursive \"{functions,lib}/**/*-test.js\"",
    "deploy": "cross-env NODE_ENV=production serverless deploy",
    "info": "serverless info",
    "format": "prettier --trailing-comma=es5 --write \"{functions,lib}/**/*.js\"",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint .",
    "logs": "serverless logs",
    "remove": "serverless remove",
    "start": "npm run watch",
    "watch": "npm-run-all --parallel watch:*",
    "watch:lint": "onchange \"{functions,lib}/**/*.js\" -v -i -p -- npm run lint",
    "watch:test": "onchange \"{functions,lib}/**/*.js\" -v -i -p -- npm run test"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.6.1",
    "eslint-plugin-mozilla": "^0.16.1",
    "eslint-plugin-no-unsanitized": "^3.0.2",
    "eslint-plugin-node": "^7.0.1",
    "mocha": "^5.2.0",
    "mock-require": "^3.0.2",
    "npm-run-all": "^4.1.3",
    "onchange": "^5.0.0",
    "prettier": "^1.14.3",
    "serverless": "^1.32.0",
    "sinon": "^6.3.4"
  },
  "dependencies": {
    "accepts": "^1.3.5",
    "aws-sdk": "^2.325.0",
    "http-signature": "^1.2.0",
    "node-fetch": "^2.2.0",
    "uuid": "^3.3.2"
  }
}
